        foreach ($input['selected_items'] as $item) {
            $real_path = realpath($item);              
            if ($real_path !== false && strpos($real_path, __DIR__) === 0) {
                                 if (is_file($real_path)) {
                    if (@unlink($real_path)) {
                        $deleted_count++;
                    }
                }
                                 elseif (is_dir($real_path)) {
                    if (@rmdir($real_path)) {
                        $deleted_count++;
                    }
                }
            }
        }
    
                 echo json_encode([
            'success' => true,
            'deleted_count' => $deleted_count
        ]);
        exit;
