         function formatSize($bytes) {
        $units = ['B', 'KB', 'MB', 'GB'];
        for ($i = 0; $i < count($units) && $bytes >= 1024; $i++) {
            $bytes /= 1024;
        }
        return round($bytes, 2) . ' ' . $units[$i];
    }

